/*--------------------------------------------------------------
1.0 - Reset
--------------------------------------------------------------*/
*,
*::before,
*::after {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

html,
body {
  height: 100%;
  scroll-behavior: smooth;
}

body {
  line-height: 1.5;
  -webkit-font-smoothing: antialiased;
}

/*--------------------------------------------------------------
2.0 - Typography
--------------------------------------------------------------*/
h1,
h2,
h3,
h4,
h5,
h6 {
  font-weight: 700;
  color: var(--text-color);
  margin-bottom: 1rem;
}

p {
  margin-bottom: 1rem;
  color: var(--text-color);
  line-height: 1.6;
}

span {
  color: var(--text-color);
}

a {
  color: var(--accent);
  text-decoration: none;
}

html {
  font-family: system-ui, sans-serif;
}

/*--------------------------------------------------------------
3.0 - Layout
--------------------------------------------------------------*/
.layout {
  display: grid;
  grid-template-areas: "main    sidebar";
  grid-template-columns: 3fr 1fr;
  gap: 1.5rem;
  min-height: 100vh;
  max-width: 72rem;
  margin: 0 auto;
  padding: 30px 15px 65px;
}

@media (max-width: 768px) {
  .layout {
    grid-template-areas:
      "main"
      "sidebar";
    grid-template-columns: 1fr;
  }
}

img {
  display: block;
  max-width: 100%;
  height: auto;
}

.featured-img{
    padding: 8px;
}

/*--------------------------------------------------------------
4.0 - Elements
--------------------------------------------------------------*/
/*--------------------------------------------------------------
4.1 - Theme Icon
--------------------------------------------------------------*/
.theme-toggle-btn {
  position: fixed;
  top: 10px;
  right: 20px;
  z-index: 2;
  border: 0;
  background: none;
  cursor: pointer;
  padding: 0;
  transition: background 0.2s;
}

.sun {
  fill: black;
}

.moon {
  fill: transparent;
}

.dark .sun {
  fill: transparent;
}

.dark .moon {
  fill: white;
}

/*--------------------------------------------------------------
5.0 Navigation
--------------------------------------------------------------*/
nav {
    position: fixed;
    top: 0;
    width: 100%;
    background: var(--bg-color);
    z-index: 2;
    display: flex;
    justify-content: space-around;
    align-items: center;
    padding: 1rem 2rem;
  }

  .logo {
    font-weight: 700;
    font-size: 1.2rem;
    color: var(--text-color);
  }

  nav ul {
    list-style: none;
    margin: 0;
    padding: 0;
  }

  .menu {
    display: flex;
    gap: 1.5rem;
    margin: auto;
  }

  .menu a {
    color: var(--text-color);
    text-decoration: none;
    position: relative;
    padding: 0.25rem 0;
    transition: color 0.3s;
  }

  /* Hover underline effect */
  .menu a::after {
    content: "";
    position: absolute;
    left: 0;
    bottom: -2px;
    width: 0;
    height: 2px;
    background: var(--accent, dodgerblue);
    transition: width 0.3s;
  }

  .menu a:hover::after,
  .menu a.active::after {
    width: 100%;
  }

  .menu a.active {
    color: var(--accent, dodgerblue);
    font-weight: 600;
  }

  /* Mobile */
  #menu-toggle {
    display: none;
  }

  .menu-icon {
    display: none;
    width: 30px;
    height: 22px;
    position: relative;
    cursor: pointer;
  }
  .menu-icon span {
    position: absolute;
    height: 3px;
    width: 100%;
    left: 0;
    background-color: var(--text-color);
    transition: 0.3s;
  }
  .menu-icon span:nth-child(1) {
    top: 0;
  }
  .menu-icon span:nth-child(2) {
    top: 9px;
  }
  .menu-icon span:nth-child(3) {
    bottom: 0;
  }

  @media (max-width: 768px) {
    nav {
      justify-content: space-between;
    }
    .logo{
      margin:auto;
    }
    li {
      padding-left: var(--spacing);
      padding-bottom: calc(var(--spacing) / 3);
    }
    .menu {
      flex-direction: column;
      gap: 1rem;
      background: var(--bg-color);
      position: absolute;
      top: 50px;
      left: 0;
      width: 100%;
      padding: 6px 2rem;
      max-height: 0;
      overflow: hidden;
      transition: max-height 0.3s ease;
    }

    #menu-toggle:checked ~ .menu {
      max-height: 300px;
    }

    .menu-icon {
      display: block;
    }

    #menu-toggle:checked + .menu-icon span:nth-child(1) {
      transform: rotate(45deg);
      top: 9px;
    }
    #menu-toggle:checked + .menu-icon span:nth-child(2) {
      opacity: 0;
    }
    #menu-toggle:checked + .menu-icon span:nth-child(3) {
      transform: rotate(-45deg);
      bottom: 9px;
    }
  }

/*--------------------------------------------------------------
6.0 - Hero Section
--------------------------------------------------------------*/
/* Hero base */
#heroSection {
  width: 100%;
  margin: 0;
  padding: 0;
}

/* Index Hero */
.index-header {
  min-height: 320px;
  padding-top: 80px; /* navbar offset */
  background: linear-gradient(90deg, var(--accent), var(--ring) 70%);
  display: flex;
  align-items: center;
  justify-content: center;
}
.index-header-title {
  font-size: 3rem;
